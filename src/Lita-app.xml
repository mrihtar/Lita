<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://ns.adobe.com/air/application/14.0">
    <id>com.dehats.sqla</id>
    <name>Lita</name> 
    <description>An administration interface for SQLite databases.</description>
    <copyright>Copyright Â© 2008 David Deraedt</copyright>
    <allowBrowserInvocation>true</allowBrowserInvocation>
    <filename>Lita</filename>
    <versionNumber>1.7.3</versionNumber>
    <initialWindow>
        <title>Lita: SQLite Database Administration</title> 
        <content>Lita.swf</content>
        <visible>true</visible>
    </initialWindow>
    <icon>
        <image16x16>assets/icons/icon_016.png</image16x16>
        <image32x32>assets/icons/icon_032.png</image32x32>
        <image48x48>assets/icons/icon_048.png</image48x48>
        <image128x128>assets/icons/icon_128.png</image128x128>
    </icon>
    <fileTypes>
        <fileType>
            <name>sqlite</name>
            <extension>sqlite</extension>
            <contentType>application/x-sqlite3</contentType>
            <description>sqlite database</description>
            <icon>
                <image16x16>assets/icons/icon_016.png</image16x16>
                <image32x32>assets/icons/icon_032.png</image32x32>
                <image48x48>assets/icons/icon_048.png</image48x48>
                <image128x128>assets/icons/icon_128.png</image128x128>
            </icon>
        </fileType>
        <fileType>
            <name>sqlite</name>
            <extension>db</extension>
            <contentType>application/x-sqlite3</contentType>
            <description>sqlite database</description>
            <icon>
                <image16x16>assets/icons/icon_016.png</image16x16>
                <image32x32>assets/icons/icon_032.png</image32x32>
                <image48x48>assets/icons/icon_048.png</image48x48>
                <image128x128>assets/icons/icon_128.png</image128x128>
            </icon>
        </fileType>
    </fileTypes>
</application>
